{
  "name": "20160802-flashcard",
  "version": "1.0.0",
  "devDependencies": {
    "array-shuffle": "^1.0.1",
    "autoprefixer": "^6.4.1",
    "axios": "^0.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "browser-sync": "^2.15.0",
    "classnames": "^2.2.5",
    "cpx": "^1.5.0",
    "csscomb": "^3.1.7",
    "director": "^1.2.8",
    "fixpack": "^2.3.1",
    "gulp": "^3.9.1",
    "gulp-postcss": "^6.2.0",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "jade-cli": "^0.1.1",
    "jscs": "^3.0.7",
    "node-sass": "^3.9.3",
    "npm-run-all": "^3.1.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "rimraf": "^2.5.4",
    "sass-lint": "^1.9.1",
    "uniqid": "^4.0.0",
    "webpack": "^1.13.2"
  },
  "keywords": [],
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "build+watch:jade": "jade ./docs_src --out ./docs --pretty --watch",
    "build+watch:js": "webpack -p --progress --watch",
    "build:scss": "gulp build:scss",
    "build:static": "cpx './docs_src/static/**/*' ./docs/static",
    "clean": "rimraf ./docs",
    "fix": "npm-run-all fix:*",
    "fix:js": "jscs ./modules ./docs_src --fix",
    "fix:pack": "fixpack",
    "fix:scss": "csscomb ./modules ./docs_src --config ./csscomb.json --verbose",
    "serve": "browser-sync start --server ./docs --files ./docs --no-ghost-mode",
    "start": "npm-run-all clean build:* -p build+watch:* watch:* serve",
    "test": "npm-run-all validate:*",
    "validate:js": "jscs ./modules ./docs_src",
    "validate:scss": "sass-lint './modules/**/*.scss, ./docs_src/**/*.scss' --config .sass-lint.yml --verbose",
    "watch:scss": "gulp watch:scss",
    "watch:static": "cpx './docs_src/static/**/*' ./docs/static --watch --no-initial"
  }
}
